{
  "name": "wordsbaking-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "tsc": "tsc",
    "tslint": "tslint",
    "stylelint": "stylelint",
    "lint:scripts": "ng lint --format verbose --type-check",
    "lint:styles": "stylelint src/**/*.less",
    "lint": "yarn lint:styles && yarn lint:scripts",
    "start": "ng serve --host 0.0.0.0 --hmr -e hmr",
    "build": "ng build -prod && node scripts/post-build.js",
    "build-dev": "ng build && node scripts/post-build.js",
    "release": "rimraf ../wordsbaking-app-cordova/www && cp -r ./bld ../wordsbaking-app-cordova/www",
    "debug-ios": "yarn build-dev && yarn release && (cd ../wordsbaking-app-cordova/www && cordova run ios)",
    "debug-android": "yarn build-dev && yarn release && (cd ../wordsbaking-app-cordova/www && cordova run android)"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.0.0",
    "@angular/common": "^5.0.0",
    "@angular/compiler": "^5.0.0",
    "@angular/core": "^5.0.0",
    "@angular/forms": "^5.0.0",
    "@angular/http": "^5.0.0",
    "@angular/platform-browser": "^5.0.0",
    "@angular/platform-browser-dynamic": "^5.0.0",
    "@angular/router": "^5.0.0",
    "@angularclass/hmr": "^2.1.3",
    "@xkeshi/image-compressor": "^0.5.2",
    "axios": "^0.17.0",
    "core-js": "^2.5.1",
    "debug": "^3.1.0",
    "extendable-error": "^0.1.5",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "memorize-decorator": "^0.2.1",
    "moment": "^2.19.2",
    "rxjs": "^5.4.3",
    "tslib": "^1.8.0",
    "villa": "^0.3.1",
    "web-animations-js": "^2.3.1",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "^1.5.0",
    "@angular/compiler-cli": "^5.0.0",
    "@angular/language-service": "^5.0.0",
    "@types/debug": "^0.0.30",
    "@types/jquery": "^3.2.16",
    "@types/lodash": "^4.14.82",
    "@types/moment": "^2.13.0",
    "@types/websql": "^0.0.27",
    "codelyzer": "^4.0.1",
    "dotenv": "^4.0.0",
    "prettier": "^1.7.4",
    "rimraf": "^2.6.2",
    "stylelint": "^8.2.0",
    "stylelint-config-standard": "^17.0.0",
    "ts-node": "~3.3.0",
    "tslint": "^5.8.0",
    "tslint-language-service": "^0.9.6",
    "typescript": "~2.4.2",
    "vts": "^5.8.3"
  }
}
